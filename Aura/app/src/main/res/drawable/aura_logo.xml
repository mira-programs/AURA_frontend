<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer circle -->
    <item android:top="-10dp" android:bottom="-10dp" android:left="-10dp" android:right="-10dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <size android:width="180dp" android:height="180dp" />
        </shape>
    </item>
    <!-- First white circle -->
    <item android:top="20dp" android:bottom="20dp" android:left="20dp" android:right="20dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
            <size android:width="80dp" android:height="80dp" />
        </shape>
    </item>
    <!-- Second holo orange dark circle -->
    <item android:top="30dp" android:bottom="30dp" android:left="30dp" android:right="30dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/holo_orange_dark" />
            <size android:width="140dp" android:height="140dp" />
        </shape>
    </item>
    <!-- Second white circle -->
    <item android:top="50dp" android:bottom="50dp" android:left="50dp" android:right="50dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
            <size android:width="60dp" android:height="60dp" />
        </shape>
    </item>
    <!-- Third holo orange dark circle -->
    <item android:top="60dp" android:bottom="60dp" android:left="60dp" android:right="60dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/holo_orange_dark" />
            <size android:width="100dp" android:height="100dp" />
        </shape>
    </item>
    <!-- Third white circle -->
    <item android:top="80dp" android:bottom="80dp" android:left="80dp" android:right="80dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
            <size android:width="40dp" android:height="40dp" />
        </shape>
    </item>
</layer-list>
