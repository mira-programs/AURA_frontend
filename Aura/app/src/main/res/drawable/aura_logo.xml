<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/holo_orange_dark" />
            <size android:width="100dp" android:height="100dp" />
        </shape>
    </item>
    <!-- First white circle -->
    <item android:top="10dp" android:bottom="10dp" android:left="10dp" android:right="10dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
        </shape>
    </item>
    <!-- Second holo orange dark circle -->
    <item android:top="30dp" android:bottom="30dp" android:left="30dp" android:right="30dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/holo_orange_dark" />
        </shape>
    </item>
    <!-- Second white circle -->
    <item android:top="40dp" android:bottom="40dp" android:left="40dp" android:right="40dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
        </shape>
    </item>
    <!-- Third holo orange dark circle -->
    <item android:top="60dp" android:bottom="60dp" android:left="60dp" android:right="60dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/holo_orange_dark" />
        </shape>
    </item>
    <!-- Third white circle -->
    <item android:top="70dp" android:bottom="70dp" android:left="70dp" android:right="70dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" />
        </shape>
    </item>
</layer-list>
